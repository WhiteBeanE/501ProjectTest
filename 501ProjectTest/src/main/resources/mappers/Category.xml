<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oracle.ProjectTest.CategoryMapper">
	<select id="CategoryList" resultType="Category">
		SELECT * FROM category
	</select>
	<select id="UpperCategoryList" resultType="Category" parameterType="int">
		SELECT * FROM category WHERE upper_category_no = #{category1Value}
	</select>
</mapper>